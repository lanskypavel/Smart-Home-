        <UserObject label="" plantUmlData="{&quot;data&quot;:&quot;@startuml\n&#39; Client Class Diagram\n\nskinparam classAttributeIconSize 0\nskinparam packageStyle rect\nhide empty members\n\npackage \&quot;Networking &amp; Communication\&quot; {\n    class ClientNetworkService {\n        -socket: Socket\n        -out: ObjectOutputStream\n        -in: ObjectInputStream\n        -serverAddress: String\n        -serverPort: int\n        -responseListener: ClientResponseListener\n        +connect()\n        +disconnect()\n        +sendRequest(request: Request)\n        -listenForServerResponses() &#39; Runs in a separate thread\n    }\n\n    interface ClientResponseListener {\n        +onResponseReceived(response: Response)\n        +onConnectionError(errorMessage: String)\n        +onDisconnected()\n    }\n\n    class Request {\n      &#39; Same as server-side, can be in a shared module\n      -type: RequestType\n      -payload: Object\n      -token: String\n    }\n    ClientNetworkService ..&gt; Request : sends\n\n    class Response {\n      &#39; Same as server-side, can be in a shared module\n      -status: ResponseStatus\n      -message: String\n      -data: Object\n    }\n    ClientNetworkService ..&gt; Response : receives\n\n    enum RequestType {\n        LOGIN\n        REGISTER\n        GET_DEVICES\n        ADD_DEVICE\n        CONTROL_DEVICE\n        GET_DEVICE_STATUS\n        CREATE_SCENE\n        EXECUTE_SCENE\n        LOGOUT\n        &#39; ...\n    }\n\n    enum ResponseStatus {\n        SUCCESS\n        ERROR\n        UNAUTHORIZED\n        NOT_FOUND\n        INVALID_REQUEST\n        &#39; ...\n    }\n}\n\npackage \&quot;User Interface (Conceptual)\&quot; {\n    abstract class BaseScreenController implements ClientResponseListener {\n        #networkService: ClientNetworkService\n        #uiManager: UIManager\n        #sessionManager: SessionManager\n\n        +show()\n        +hide()\n        #sendRequestToServer(request: Request)\n        +onResponseReceived(response: Response) &#39; abstract or with default impl\n        +onConnectionError(errorMessage: String)\n        +onDisconnected()\n    }\n\n    class LoginScreenController extends BaseScreenController {\n        -usernameField: TextField\n        -passwordField: PasswordField\n        +handleLoginButton()\n        +handleRegisterNavigation()\n        +onResponseReceived(response: Response) &#39; Processes LOGIN_RESPONSE\n    }\n\n    class RegisterScreenController extends BaseScreenController {\n        &#39; ... fields for registration\n        +handleRegisterButton()\n        +onResponseReceived(response: Response) &#39; Processes REGISTER_RESPONSE\n    }\n\n    class MainDashboardController extends BaseScreenController {\n        -devicesListView: ListView&lt;DeviceUIModel&gt;\n        -scenesListView: ListView&lt;SceneUIModel&gt;\n        +loadUserDevices()\n        +loadUserScenes()\n        +handleDeviceControl(deviceId: String, action: String, value: any)\n        +handleAddDeviceButton()\n        +handleCreateSceneButton()\n        +handleLogoutButton()\n        +onResponseReceived(response: Response) &#39; Processes device lists, statuses, etc.\n    }\n    &#39; ... other screen controllers (DeviceDetailsScreen, SceneCreationScreen, etc.)\n\n    class UIManager {\n        -primaryStage: Stage\n        -currentController: BaseScreenController\n        +showLoginScreen()\n        +showMainDashboard()\n        +showError(title: String, message: String)\n        +showInfo(title: String, message: String)\n        &#39; ... navigation methods\n    }\n    BaseScreenController ..&gt; UIManager : uses\n}\n\npackage \&quot;Client-Side Models &amp; State Management\&quot; {\n    class SessionManager {\n        -currentUser: UserClientModel\n        -authToken: String\n        +setCurrentUser(user: UserClientModel, token: String)\n        +getCurrentUser(): UserClientModel\n        +getAuthToken(): String\n        +isUserLoggedIn(): boolean\n        +clearSession()\n    }\n    BaseScreenController ..&gt; SessionManager : uses\n\n    class UserClientModel {\n        &#39; Simplified representation for client\n        -username: String\n        -email: String\n    }\n\n    class DeviceUIModel {\n        &#39; Representation for UI display\n        -deviceId: String\n        -deviceName: String\n        -deviceType: String\n        -statusText: String\n        -isControllable: boolean\n    }\n    MainDashboardController ..&gt; DeviceUIModel : displays\n\n    class SceneUIModel {\n        -sceneId: String\n        -sceneName: String\n    }\n    MainDashboardController ..&gt; SceneUIModel : displays\n}\n\n\nClientNetworkService ..&gt; ClientResponseListener : notifies &gt;\nnote \&quot;UI Package is conceptual. \\nSpecific UI framework (JavaFX, Android, etc.) \\nwill dictate actual class names and structure.\&quot; as N_UI\nnote \&quot;Request/Response classes could be in a \\nshared library between client and server.\&quot; as N_Shared\n\n@enduml&quot;,&quot;format&quot;:&quot;svg&quot;}" id="FIs-bcYHAyVkWWkcSstQ-1">
