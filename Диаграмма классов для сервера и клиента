        <UserObject label="" plantUmlData="{&quot;data&quot;:&quot;@startuml\n&#39; Server Class Diagram\n\nskinparam classAttributeIconSize 0\nskinparam packageStyle rect\nhide empty members\n\npackage \&quot;Core &amp; Networking\&quot; {\n    class Server {\n        -serverSocket: ServerSocket\n        -executorService: ExecutorService\n        -clients: List&lt;ClientHandler&gt;\n        -dbHandler: DatabaseHandler\n        -userManager: UserManager\n        -deviceManager: DeviceManager\n        +start()\n        +stop()\n        +broadcastMessage(message: String, sender: ClientHandler)\n        +removeClient(clientHandler: ClientHandler)\n        +processClientRequest(request: Request, handler: ClientHandler): Response\n    }\n\n    class ClientHandler implements Runnable {\n        -clientSocket: Socket\n        -in: ObjectInputStream\n        -out: ObjectOutputStream\n        -server: Server\n        -currentUser: User\n        +run()\n        -handleRequest(request: Request): Response\n        -authenticateUser(credentials: Credentials): User\n        -registerUser(userData: UserData): boolean\n        -sendMessageToClient(response: Response)\n        +disconnect()\n        +getCurrentUser(): User\n    }\n\n    Server \&quot;1\&quot; *-- \&quot;0..*\&quot; ClientHandler : manages &gt;\n    ClientHandler ..&gt; Server : uses\n}\n\npackage \&quot;Data Handling &amp; Business Logic\&quot; {\n    class DatabaseHandler {\n        -connection: Connection\n        +connect()\n        +disconnect()\n        +saveUser(user: User): boolean\n        +getUser(username: String): User\n        +updateUser(user: User)\n        +saveDevice(device: Device): boolean\n        +getDevice(deviceId: String): Device\n        +getAllDevices(userId: String): List&lt;Device&gt;\n        +updateDeviceState(deviceId: String, state: String)\n        +removeDevice(deviceId: String)\n        &#39; ... other DB operations (logs, scenes etc.)\n    }\n    Server --&gt; DatabaseHandler : uses &gt;\n\n    class UserManager {\n        -dbHandler: DatabaseHandler\n        +authenticate(username: String, password: String): User\n        +register(username: String, password: String, email: String): boolean\n        +getUserProfile(username: String): User\n        +updateUserProfile(user: User)\n    }\n    Server --&gt; UserManager : uses &gt;\n    UserManager ..&gt; DatabaseHandler : uses\n\n    class DeviceManager {\n        -dbHandler: DatabaseHandler\n        +addDevice(userId: String, deviceType: String, deviceName: String): Device\n        +removeDevice(deviceId: String)\n        +getDeviceStatus(deviceId: String): String\n        +controlDevice(deviceId: String, action: String, value: any): boolean\n        +getAllUserDevices(userId: String): List&lt;Device&gt;\n        +createScene(scene: SceneData): boolean\n        +executeScene(sceneId: String)\n    }\n    Server --&gt; DeviceManager : uses &gt;\n    DeviceManager ..&gt; DatabaseHandler : uses\n}\n\npackage \&quot;Models &amp; Communication\&quot; {\n    class User {\n        -userId: String\n        -username: String\n        -hashedPassword: String\n        -email: String\n        -devices: List&lt;Device&gt;\n    }\n    UserManager ..&gt; User : creates/manages\n\n    abstract class Device {\n        -deviceId: String\n        -deviceName: String\n        -deviceType: String\n        -status: String &#39; e.g., \&quot;ON\&quot;, \&quot;OFF\&quot;, \&quot;level:50%\&quot;\n        -ownerId: String\n        +turnOn()\n        +turnOff()\n        +getStatus(): String\n        +updateState(newState: String)\n    }\n    DeviceManager ..&gt; Device : creates/manages\n\n    class Light extends Device {\n        -brightness: int\n        -color: String\n        +setBrightness(level: int)\n        +setColor(color: String)\n    }\n\n    class Thermostat extends Device {\n        -currentTemperature: double\n        -targetTemperature: double\n        +setTargetTemperature(temp: double)\n    }\n    &#39; ... other device types (Socket, Camera etc.)\n\n    class Request {\n        -type: RequestType &#39; e.g., LOGIN, REGISTER, GET_DEVICES, CONTROL_DEVICE\n        -payload: Object &#39; Contains data specific to request type\n        -token: String &#39; Optional: for authenticated requests\n    }\n    ClientHandler ..&gt; Request : receives\n\n    class Response {\n        -status: ResponseStatus &#39; e.g., SUCCESS, ERROR, UNAUTHORIZED\n        -message: String\n        -data: Object &#39; Contains data specific to response\n    }\n    ClientHandler ..&gt; Response : sends\n\n    enum RequestType {\n        LOGIN\n        REGISTER\n        GET_DEVICES\n        ADD_DEVICE\n        CONTROL_DEVICE\n        GET_DEVICE_STATUS\n        CREATE_SCENE\n        EXECUTE_SCENE\n        LOGOUT\n        &#39; ...\n    }\n\n    enum ResponseStatus {\n        SUCCESS\n        ERROR\n        UNAUTHORIZED\n        NOT_FOUND\n        INVALID_REQUEST\n        &#39; ...\n    }\n}\n\nnote \&quot;Focus on key components. \\nFurther details like Scene, Log models and their \\nmanagement can be added.\&quot; as N1\n\n@enduml&quot;,&quot;format&quot;:&quot;svg&quot;}" id="UFRjJEOazWDJc5YvBh99-1">
